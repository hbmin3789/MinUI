<Application x:Class="MinUI.Demo.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MinUI.Demo"
             StartupUri="MainWindow.xaml"
			 xmlns:util="clr-namespace:MinUI.Demo.Utils">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="pack://application:,,,/MinUI.Demo;component/Themes/Templates.xaml" />
			</ResourceDictionary.MergedDictionaries>
			<util:ViewSelector x:Key="ViewSelector" />
		</ResourceDictionary>
	</Application.Resources>
</Application>
