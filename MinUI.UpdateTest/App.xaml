             xmlns:local="clr-namespace:MinUI.UpdateTest"
			 xmlns:view="clr-namespace:MinUI.UpdateTest.Util"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="pack://application:,,,/MinUI.UpdateTest;component/View/ViewSelector.xaml" />
			</ResourceDictionary.MergedDictionaries>
