<UserControl x:Class="MinUI.Test.Controls.ChartsControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MinUI.Test.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
			 xmlns:chart="clr-namespace:MinUI.Core.Charts;assembly=MinUI.Core">
    <Grid>
		<chart:BarChart x:Name="bc"
						Grid.Row="1"
						Title="Test Chart"
						DatasSource="{Binding BarChartDatas}" />
	</Grid>
</UserControl>
